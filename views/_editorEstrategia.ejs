<div class="form-group row">
  <label class="col-form-label col-xs-2 col-md-2" style="text-align: right;" for="Descricao">Descrição</label>
  <div class="col-xs-10 col-md-6">
    <input autofocus class="form-control" name="Descricao" value="<%= model.Descricao %>" />
  </div>
</div>


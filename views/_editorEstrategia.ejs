  <label class="col-form-label col-xs-2 col-md-2" style="text-align: right;" for="TempoJogoInicial">Tempo Jogo</label>
  <div class="col-xs-6 col-md-1">
    <input class="form-control" name="TempoJogoInicial" value="<%= model.TempoJogoInicial %>" min="0" max="90" type="number"/>
  </div> <span> a </span> 
  <div class="col-xs-6 col-md-1">
    <input class="form-control" name="TempoJogoFinal" value="<%= model.TempoJogoFinal %>" min="0" max="90" type="number"/>
  </div>
</div>

